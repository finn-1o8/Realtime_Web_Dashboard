    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "three": "^0.158.0",
    "@react-three/fiber": "^8.15.11",
    "@react-three/drei": "^9.88.13",
    "recharts": "^2.10.3",
    "socket.io-client": "^4.6.1",
